							option(value="N") N
					td 
						select#row4least			
							option(value="Z" selected) Z
							option(value="SQ") SQ
							option(value="TRI") TRI
							option(value="STAR") STAR
							option(value="N") N
		input#pageno(type="hidden" value=pagenum)					
		input#submit(type="submit" value="submit") 					