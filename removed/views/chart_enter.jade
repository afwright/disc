extends layout
block content
	p value #{value}
	form#chartenter(method="post" action="/admin/update_chart")
		table#mocktable
			thead
				tr
					th D
					th I
					th S
					th C
			tbody
				tr
					td
						input#D_in(name="D" type="text")
					td
						input#I_in(name="I" type="text")
					td
						input#S_in(name="S" type="text")
					td
						input#C_in(name="C" type="text")
		input#val(name="val" type="hidden" value="#{value}")				
		input#submit(type="submit" value="submit")